class test:
    def __init__(self):
        self.sam="sush"

    def sample(self,name,roll,batch,grade):
        self.name=name
        self.roll=roll
        self.batch=batch
        self.grade=grade

#object creation
#t=test()
vamshee=test()
sush=test()

sush.sample("sush",2,'B4',8.5)
vamshee.sample("vamshee",1,"B3",9)

sush.sam2()





